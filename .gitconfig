[user]
        name = Kevin Orr
        useConfigOnly = true
[alias]
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto

[push]
        default = current
[pull]
        rebase = merges
[diff]
	algorithm = patience
